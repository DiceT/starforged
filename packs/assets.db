rforged.starforged-moves\" data-id='koezMI2GxypVUqy2' style='display: inline-block'><i class=\"fas fa-suitcase\"></i> Face Desolation</a>."}},"track":{"enabled":false,"value":5,"min":0,"max":5},"battered":{"enabled":false,"value":false},"cursed":{"enabled":false,"value":false},"outOfAction":{"enabled":false,"value":false}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"RxgzdFrkzL6ATDW4":3},"flags":{}}
